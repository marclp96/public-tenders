{
  "schemaVersion": 1,
  "type": "object",
  "title": "Tender Scraper Input",
  "description": "Config for Supabase + XML source",
  "properties": {
    "supabaseKey": {
      "type": "string",
      "title": "Supabase Service Key",
      "editor": "textfield",
      "description": "Paste your Supabase service role key here"
