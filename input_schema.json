    "supabaseUrl": {
      "type": "string",
      "title": "Supabase REST API URL",
      "editor": "textfield",
      "description": "e.g., https://your-project.supabase.co/rest/v1/tenders"
    },
    "feedUrl": {
      "type": "string",
      "title": "XML Feed URL",
      "editor": "textfield",
      "default": "https://contrataciondelestado.es/sindicacion/sindicacion_31500000_1.xml",
      "description": "URL of the XML feed (CPV 31500000)"
    }
  },
  "required": ["supabaseKey", "supabaseUrl"]
}
